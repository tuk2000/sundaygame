                                            </children>
                                        </AnchorPane>
                                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
                                            <children>
                                                <TitledPane animated="false" layoutX="23.0" layoutY="131.0"
                                                            prefHeight="527.0" prefWidth="344.0" text="Mouse"
                                                            AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                                            AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                                    <content>
                                                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0"
                                                                    prefWidth="200.0">
                                                            <children>
                                                                <Label layoutX="48.0" layoutY="50.0" text="Signal"/>
                                                                <Label layoutX="48.0" layoutY="103.0" text="Key"/>
                                                                <Label fx:id="mouseSignal" layoutX="134.0"
                                                                       layoutY="50.0" text="None"/>
                                                                <Label fx:id="mouseKey" layoutX="141.0" layoutY="103.0"
                                                                       text="......"/>
                                                                <Label layoutX="48.0" layoutY="151.0" text="Status"/>
                                                                <Label fx:id="mouseStatus" layoutX="141.0"
                                                                       layoutY="151.0" text="......"/>
                                                            </children>
                                                        </AnchorPane>
                                                    </content>
                                                </TitledPane>
                                            </children>
                                        </AnchorPane>
                                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="527.0" prefWidth="789.0">
                                            <children>
                                                <TitledPane animated="false" layoutX="26.0" layoutY="98.0"
                                                            prefHeight="527.0" prefWidth="358.0" text="GamePad"
                                                            AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                                            AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
