                                           AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                    <columns>
                                        <TableColumn prefWidth="84.0" sortable="false" text="Type"/>
                                        <TableColumn prefWidth="79.5" sortable="false" text="Tag"/>
                                        <TableColumn prefWidth="209.0" sortable="false" text="Content"/>
                                    </columns>
                                </TableView>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
