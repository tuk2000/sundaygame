            <tabs>
                <Tab text="GameMonitor">
                    <content>
                        <SplitPane dividerPositions="0.5" orientation="VERTICAL">
                            <items>
                                <StackedAreaChart fx:id="memoryChart" animated="false" prefHeight="400.0"
                                                  prefWidth="385.0" title="MemoryUsage">
                                    <xAxis>
                                        <NumberAxis label="Time(S)" minorTickCount="10" side="BOTTOM">
                                            <effect>
                                                <Blend/>
                                            </effect>
                                        </NumberAxis>
                                    </xAxis>
                                    <yAxis>
                                        <NumberAxis label="KB" minorTickCount="16" prefHeight="188.0" prefWidth="60.0"
                                                    side="LEFT" upperBound="102400.0"/>
                                    </yAxis>
                                </StackedAreaChart>
                                <StackedAreaChart fx:id="fpsChart" animated="false" title="FPS">
                                    <xAxis>
                                        <NumberAxis label="Time(S)" minorTickCount="10" side="BOTTOM">
                                            <effect>
                                                <Blend/>
                                            </effect>
                                        </NumberAxis>
                                    </xAxis>
                                    <yAxis>
                                        <NumberAxis label="FPS" prefHeight="187.0" prefWidth="60.0" side="LEFT"
                                                    tickLabelGap="1.0" upperBound="80.0"/>
                                    </yAxis>
                                </StackedAreaChart>
                            </items>
                        </SplitPane>
                    </content>
                </Tab>
                <Tab text="Screen">
                    <content>
                        <SplitPane dividerPositions="0.25" nodeOrientation="LEFT_TO_RIGHT">
                            <items>
                                <SplitPane dividerPositions="0.7" orientation="VERTICAL" prefHeight="529.0"
                                           prefWidth="227.0">
                                    <items>
                                        <ListView fx:id="screens" prefHeight="529.0" prefWidth="210.0"
                                                  AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                                  AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
                                        <Button fx:id="btLoad" alignment="CENTER" contentDisplay="TOP"
                                                mnemonicParsing="false" onAction="#loadScreen" text="Load"/>
                                    </items>
                                </SplitPane>
                                <TableView fx:id="screenTable" prefHeight="529.0" prefWidth="570.0">
                                    <columns>
                                        <TableColumn prefWidth="75.0" text="Class"/>
                                        <TableColumn prefWidth="75.0" text="Instances"/>
                                    </columns>
                                </TableView>
                            </items>
                        </SplitPane>
                    </content>
                </Tab>
                <Tab text="Log">
                    <content>
                        <AnchorPane>
                            <children>
                                <TableView fx:id="logTable" layoutX="14.0" layoutY="14.0" prefHeight="529.0"
