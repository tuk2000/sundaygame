                                                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0"
                                                                    prefWidth="200.0">
                                                            <children>
                                                                <Label layoutX="53.0" layoutY="48.0" text="Signal"/>
                                                                <Label layoutX="53.0" layoutY="101.0" text="Key"/>
                                                                <Label layoutX="53.0" layoutY="150.0" text="Status"/>
                                                                <Label fx:id="gamePadSignal" layoutX="139.0"
                                                                       layoutY="48.0" text="None"/>
                                                                <Label fx:id="gamePadKey" layoutX="146.0"
                                                                       layoutY="101.0" text="......"/>
                                                                <Label fx:id="gamePadStatus" layoutX="146.0"
                                                                       layoutY="150.0" text="......"/>
                                                            </children>
                                                        </AnchorPane>
                                                    </content>
                                                </TitledPane>
                                            </children>
                                        </AnchorPane>
                                    </items>
                                </SplitPane>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab text="Data">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <VBox layoutX="93.0" layoutY="14.0" prefHeight="529.0" prefWidth="1063.0"
                                      AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                      AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                    <children>
                                        <TreeTableView fx:id="treeTableView">
                                            <columns>
                                                <TreeTableColumn prefWidth="299.0" text="Group"/>
                                                <TreeTableColumn minWidth="0.0" prefWidth="763.0" text="Instance"/>
                                            </columns>
                                        </TreeTableView>
                                        <HBox>
                                            <children>
                                                <RadioButton fx:id="radixOptSystem" alignment="CENTER_LEFT"
                                                             mnemonicParsing="false" selected="true" text="System">
                                                    <toggleGroup>
                                                        <ToggleGroup fx:id="dataOptionGroup"/>
                                                    </toggleGroup>
                                                </RadioButton>
                                                <RadioButton fx:id="radixOptType" alignment="CENTER_RIGHT"
                                                             mnemonicParsing="false" text="DataType"
                                                             toggleGroup="$dataOptionGroup"/>
                                            </children>
                                        </HBox>
                                    </children>
                                </VBox>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </children>

</AnchorPane>
