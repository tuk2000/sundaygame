                                                                    prefWidth="200.0">
                                                            <children>
                                                                <TextArea fx:id="keyBoardHistory" layoutX="14.0"
                                                                          layoutY="265.0" prefHeight="200.0"
                                                                          prefWidth="305.0"/>
